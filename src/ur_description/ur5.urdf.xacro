  <gazebo>
    <plugin name="gazebo_ros_vacuum_gripper4" filename="libgazebo_ros_vacuum_gripper.so">
      <robotNamespace>/ur5/vacuum_gripper4</robotNamespace>
      <bodyName>vacuum_gripper4</bodyName>
      <topicName>grasping4</topicName>
